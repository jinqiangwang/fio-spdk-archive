[global]
percentile_list=${percentile_list}
ioengine=${ioengine}
direct=1
thread
group_reporting

[fio_job_configs]
name=seqrw73_${bs}_j${numjobs}_qd${iodepth}
rw=rw
rwmixread=70
bs=${bs}
numjobs=${numjobs}
iodepth=${iodepth}
runtime=${runtime}
ramp_time=${ramp_time}
size=100%
time_based
log_avg_msec=1000
per_job_logs=0
write_bw_log=${output_name}
write_iops_log=${output_name}
write_lat_log=${output_name}